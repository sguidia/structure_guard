<h4>Nests</h4>

<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>Nest run</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @nests.each do |nest| %>
      <tr>
        <td><%= nest.id %></td>
        <td><%= nest.nest_run_id %></td>
        <td class="text-right"><%= link_to 'Show', nest, :class => "btn btn-xs btn-default" %> <%= link_to 'Edit', edit_nest_path(nest), :class => "btn btn-xs btn-primary" %> <%= link_to 'Remove', nest, method: :delete, :class => "btn btn-xs btn-danger", data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
