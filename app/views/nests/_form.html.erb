<%= simple_form_for [@nest_run, @nest] do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
  <%= f.label :is_manufactured %><br>
  <%= f.check_box :is_manufactured, {}, {:class => 'form-control'} %>
  </div>

  <div class="form-group">
    <%= f.submit "Submit", :data => {'loading-text' => 'Submitting...'}, :class => 'btn btn-lg btn-success' %>
  </div>
<% end %>
