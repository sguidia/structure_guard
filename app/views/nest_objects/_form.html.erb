<%= simple_form_for(@nest_object) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.label :part_id %><br>
    <%= f.collection_select :part_id, Part.all, :id, :id, {:include_blank => '- select part -'}, {:class => 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.label :nest_id %><br>
    <%= f.collection_select :nest_id, Nest.all, :id, :id, {:include_blank => '- select nest -'}, {:class => 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.label :x %><br>
    <%= f.text_field :x, :type => 'number', :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :y %><br>
    <%= f.text_field :y, :type => 'number', :class => 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit "Submit", :data => {'loading-text' => 'Submitting...'}, :class => 'btn btn-lg btn-success' %>
  </div>
<% end %>
